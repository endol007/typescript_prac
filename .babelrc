{
    "presets": [
        "@babel/preset-env", 
        "@babel/preset-react", 
        "@babel/preset-typescript" //프리셋
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-arrow-functions",
        ["@babel/plugin-proposal-class-properties", { "loose": false }]
    ],
    "assumptions": {
        "setPublicClassFields": false
    }
}